{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HandshakeResponse",
  "type": "object",
  "required": ["serverVersion", "protocolVersions", "capabilities"],
  "properties": {
    "serverVersion": { "type": "string" },
    "protocolVersions": {
      "type": "array",
      "items": { "type": "string" },
      "minItems": 1
    },
    "capabilities": {
      "type": "array",
      "items": { "type": "string" }
    }
  }
}
